{
  "name": "events-manager",
  "engines": {
    "node": "6.11.2"
  },
  "description": "manage applications to your event facilities",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel ./server -d dist",
    "dev": "./node_modules/.bin/nodemon ./server/app --exec ./node_modules/.bin/babel-node",
    "eslint": "./node_modules/.bin/eslint ./server/app.js",
    "nodemon": "nodemon ./server/app",
    "test:db": "cross-env NODE_ENV=test sequelize db:migrate:undo:all &&cross-env NODE_ENV=test sequelize db:migrate",
    "test": "npm run test:db &&cross-env NODE_ENV=test nyc --reporter=lcov --reporter=html --reporter=text mocha --compilers js:babel-core/register ./server/test/**/*.spec.js --timeout 250000 --exit",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "seed:create": "sequelize seed:generate --name",
    "seed": "sequelize db:seed:all",
    "seed:undo": "sequelize db:seed:undo:all",
    "coverage": "NODE_ENV=test nyc report --reporter=text-lcov | coveralls",
    "start": "./node_modules/.bin/babel-node ./server/app",
    "client": "webpack-dev-server --config webpack.config.dev.js",
    "heroku-postbuild": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "heroku-api": "./node_modules/.bin/babel ./server -d dist && node dist/app.js",
    "webpack": "webpack -p --config webpack.config.dev.js",
    "brunch": "brunch build -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d3mola/Events-Manager.git"
  },
  "keywords": [
    "express",
    "postgres",
    "sequelize",
    "node"
  ],
  "author": "Ademola Ariya",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/d3mola/Events-Manager/issues"
  },
  "homepage": "https://github.com/d3mola/Events-Manager#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0-alpha.6",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "jsonwebtoken": "^8.1.0",
    "mocha": "^4.0.1",
    "morgan": "^1.9.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.12.1",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "react": "^16.1.1",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.1.1",
    "react-navigation": "^1.0.0-beta.21",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.5.0",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.22.7",
    "sequelize-cli": "^3.0.0",
    "style-loader": "^0.19.0",
    "supertest": "^3.0.0",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.1",
    "webpack-dev-server": "^2.9.4"
  },
